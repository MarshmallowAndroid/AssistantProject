<windows98:Windows98Window x:Class="GooeyWpf.LoadingWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:GooeyWpf"
        xmlns:windows98="clr-namespace:Windows98;assembly=Windows98"
        mc:Ignorable="d"
        Title="Loading..." ResizeMode="NoResize" WindowStartupLocation="CenterScreen" Width="300" SizeToContent="Height" Loaded="Window_Loaded">
    <Grid>
        <StackPanel Orientation="Horizontal">
            <Image Style="{StaticResource DrumsAnimation}" HorizontalAlignment="Left" VerticalAlignment="Center" Margin="8,8" />
        <Label x:Name="label" Content="Initializing components..." HorizontalAlignment="Left" VerticalAlignment="Center" />
        </StackPanel>
    </Grid>
</windows98:Windows98Window>